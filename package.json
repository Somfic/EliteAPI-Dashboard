{
  "name": "eliteapi.dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "dotnet publish src-server -c Release -a x64 -o src-tauri/bin/eliteapi",
    "postbuild": "node --experimental-modules scripts/rename-arch.js",
    "build": "rollup -c",
    "dev": "concurrently npm:dev:*",
    "dev:server": "dotnet watch --project src-server",
    "dev:client": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tauri": "tauri"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tauri-apps/cli": "^1.0.5",
    "@tsconfig/svelte": "^2.0.0",
    "concurrently": "^7.3.0",
    "execa": "^6.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.54.8",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2",
    "download-github-release": "^0.3.2",
    "fuzzysort": "^2.0.1",
    "sirv-cli": "^2.0.0"
  }
}
